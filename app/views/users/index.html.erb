<%= render 'list', book: @book, user: @user %>

<h1>Users</h1>
<table>
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <% @users.each do |user| %>
    <tbody>
      <tr>
        <th><%= image_tag user.get_profile_image(100,100) %></th>
        <th><%= user.name %></th>
        <th><%= link_to "show", user_path(user.id) %></th>
      </tr>
    </tbody>
  <% end %>
</table>